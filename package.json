{
  "name": "hamptons-home-management",
  "version": "1.0.0",
  "description": "Hamptons Home Management Website",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "analyze": "cross-env NODE_ENV=production NODE_OPTIONS=\"--no-deprecation\" next experimental-analyze",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation eslint .",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit --incremental",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_ENV=production NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "3.73.0",
    "@payloadcms/live-preview-react": "3.73.0",
    "@payloadcms/next": "3.73.0",
    "@payloadcms/payload-cloud": "3.73.0",
    "@payloadcms/richtext-lexical": "3.73.0",
    "@payloadcms/storage-vercel-blob": "3.73.0",
    "@payloadcms/ui": "3.73.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cross-env": "10.1.0",
    "date-fns": "4.1.0",
    "focus-trap-react": "11.0.6",
    "libphonenumber-js": "1.12.35",
    "lodash-es": "4.17.23",
    "lucide-react": "0.563.0",
    "next": "16.1.4",
    "next-recaptcha-v3": "1.5.3",
    "payload": "3.73.0",
    "payload-totp": "3.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-use": "17.6.0",
    "server-only": "0.0.1",
    "sharp": "0.34.5",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@tailwindcss/postcss": "4.1.18",
    "@tailwindcss/typography": "0.5.19",
    "@types/jsdom": "27.0.0",
    "@types/lodash-es": "4.17.12",
    "@types/node": "25.0.10",
    "@types/nprogress": "0.2.3",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@types/react-virtualized": "9.22.3",
    "eslint": "9.39.2",
    "eslint-plugin-react-hooks": "7.0.1",
    "globals": "17.1.0",
    "postcss": "8.5.6",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ],
    "ignoredBuiltDependencies": [
      "@tailwindcss/oxide"
    ]
  }
}
